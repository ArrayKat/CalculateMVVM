<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Calculator.Calculate">


	<StackPanel>
		<TextBlock>Добро пожаловать в калькулятор!</TextBlock>
		<TextBlock>Введите арифметическую операцию: </TextBlock>
		<StackPanel Orientation="Horizontal">
			<TextBox Height="20" Width="160" Margin="10"></TextBox>
			<ComboBox Margin="10"/>
			<TextBox Height="20" Width="160" Margin="10"></TextBox>
		</StackPanel>
		<Button>Расчитать</Button>
		<TextBlock>Результат: </TextBlock>
	</StackPanel>
	
	
	
</UserControl>
